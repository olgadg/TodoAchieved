<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.example.olgadominguez.todoachieved">

  <application
    android:name=".TodoApplication"
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">

    <activity android:name="com.example.olgadominguez.todoachieved.task.list.TaskListActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name="com.example.olgadominguez.todoachieved.task.form.TaskFormActivity" />


  </application>

</manifest>
